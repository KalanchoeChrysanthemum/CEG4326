package main

import (
    "strconv"
    "time"
    "fmt"
)

func main() {
    t := time.Now().Unix()
    fmt.Println("Time: ", t)
    pt := "123456"

    stamp, err := strconv.Atoi(pt)
    
    if err != nil {
	fmt.Println("Failed to convert")
    }

    if t - int64(stamp) > 10 {
	fmt.Println("Expired timestamp")
    } else {
	fmt.Println("Valid timestamp")
    }
}
